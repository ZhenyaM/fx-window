<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.Label?>
<?import java.lang.Double?>
<AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
            styleClass="uf_win_border"
            xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1"
            fx:id="rootPane"
            fx:controller="com.fx.window.controller.WindowController"
>
    <fx:define>
        <Double fx:id="DEFAULT_RESIZE_SIZE" fx:value="3.0"/>
    </fx:define>
    <fx:include source="/fxml/menu-bar.fxml"
                fx:id="menuBar"
                AnchorPane.leftAnchor="$DEFAULT_RESIZE_SIZE"
                AnchorPane.rightAnchor="$DEFAULT_RESIZE_SIZE"
                AnchorPane.topAnchor="$DEFAULT_RESIZE_SIZE"/>
    <Label fx:id="windowTitle"
            mouseTransparent="true" alignment="CENTER"
            AnchorPane.leftAnchor="$DEFAULT_RESIZE_SIZE"
           AnchorPane.rightAnchor="$DEFAULT_RESIZE_SIZE"
           AnchorPane.topAnchor="$DEFAULT_RESIZE_SIZE">
        <text>Test title for test purposes</text>
    </Label>
    <Region fx:id="winResizeN" prefHeight="$DEFAULT_RESIZE_SIZE"
            AnchorPane.leftAnchor="$DEFAULT_RESIZE_SIZE"
            AnchorPane.rightAnchor="$DEFAULT_RESIZE_SIZE"
            AnchorPane.topAnchor="0.0">
        <cursor>
            <Cursor fx:constant="N_RESIZE"/>
        </cursor>
    </Region>
    <Region fx:id="winResizeE" layoutX="40.0" layoutY="40.0"
            prefWidth="$DEFAULT_RESIZE_SIZE"
            AnchorPane.bottomAnchor="$DEFAULT_RESIZE_SIZE"
            AnchorPane.rightAnchor="0.0"
            AnchorPane.topAnchor="$DEFAULT_RESIZE_SIZE">
        <cursor>
            <Cursor fx:constant="E_RESIZE"/>
        </cursor>
    </Region>
    <Region fx:id="winResizeS" layoutX="60.0" layoutY="60.0"
            prefWidth="$DEFAULT_RESIZE_SIZE"
            prefHeight="$DEFAULT_RESIZE_SIZE"
            AnchorPane.bottomAnchor="0.0"
            AnchorPane.leftAnchor="$DEFAULT_RESIZE_SIZE"
            AnchorPane.rightAnchor="$DEFAULT_RESIZE_SIZE">
        <cursor>
            <Cursor fx:constant="S_RESIZE"/>
        </cursor>
    </Region>
    <Region fx:id="winResizeSW" layoutX="70.0" layoutY="70.0"
            prefWidth="$DEFAULT_RESIZE_SIZE"
            prefHeight="$DEFAULT_RESIZE_SIZE"
            AnchorPane.bottomAnchor="0.0"
            AnchorPane.leftAnchor="0.0">
        <cursor>
            <Cursor fx:constant="SW_RESIZE"/>
        </cursor>
    </Region>
    <Region fx:id="winResizeW" layoutX="20.0" layoutY="20.0"
            prefWidth="$DEFAULT_RESIZE_SIZE"
            AnchorPane.bottomAnchor="$DEFAULT_RESIZE_SIZE"
            AnchorPane.leftAnchor="0.0"
            AnchorPane.topAnchor="$DEFAULT_RESIZE_SIZE">
        <cursor>
            <Cursor fx:constant="W_RESIZE"/>
        </cursor>
    </Region>
    <Region fx:id="winResizeSE" layoutX="50.0" layoutY="50.0"
            prefWidth="$DEFAULT_RESIZE_SIZE"
            prefHeight="$DEFAULT_RESIZE_SIZE"
            AnchorPane.bottomAnchor="0.0"
            AnchorPane.rightAnchor="0.0">
        <cursor>
            <Cursor fx:constant="SE_RESIZE"/>
        </cursor>
    </Region>
    <Region fx:id="winResizeNE" layoutX="30.0" layoutY="30.0"
            prefWidth="$DEFAULT_RESIZE_SIZE"
            prefHeight="$DEFAULT_RESIZE_SIZE"
            AnchorPane.rightAnchor="0.0"
            AnchorPane.topAnchor="0.0">
        <cursor>
            <Cursor fx:constant="NE_RESIZE"/>
        </cursor>
    </Region>
    <Region fx:id="winResizeNW" layoutX="10.0" layoutY="10.0"
            prefHeight="$DEFAULT_RESIZE_SIZE"
            prefWidth="$DEFAULT_RESIZE_SIZE"
            AnchorPane.leftAnchor="0.0"
            AnchorPane.topAnchor="0.0">
        <cursor>
            <Cursor fx:constant="NW_RESIZE"/>
        </cursor>
    </Region>
</AnchorPane>
